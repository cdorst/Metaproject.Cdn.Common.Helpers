<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>cdorst</Authors>
    <Description>Helpers for downloading content from the cdorst-dev CDN</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>CDorst.Metaproject.Cdn.Common.Helpers</PackageId>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CDorst.DevOps.ContentDelivery.Functions.DownloadAndExtractAzureCdnBlobZip" Version="1.0.0" />
    <PackageReference Include="CDorst.DevOps.ContentDelivery.Functions.DownloadAzureCdnBlob" Version="1.0.0" />
  </ItemGroup>

</Project>
